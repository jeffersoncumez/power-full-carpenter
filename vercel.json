{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "frontend/dist" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "https://power-full-carpenter.onrender.com/api/$1" },
    { "src": "/(.*)", "dest": "/frontend/dist/index.html" }
  ]
}
